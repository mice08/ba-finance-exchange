<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    <bean id="invoiceRecoDao" parent="parentDao">
        <property name="proxyInterfaces" value="com.dianping.ba.finance.exchange.midasreco.biz.dao.InvoiceRecoDao"/>
        <property name="target">
            <bean parent="fsMain.daoRealizeTarget">
                <constructor-arg value="InvoiceReco"/>
            </bean>
        </property>
    </bean>

    <bean id="receiveOrderRecoDao" parent="parentDao">
        <property name="proxyInterfaces" value="com.dianping.ba.finance.exchange.midasreco.biz.dao.ReceiveOrderRecoDao"/>
        <property name="target">
            <bean parent="fsMain.daoRealizeTarget">
                <constructor-arg value="ReceiveOrderReco"/>
            </bean>
        </property>
    </bean>
</beans>
