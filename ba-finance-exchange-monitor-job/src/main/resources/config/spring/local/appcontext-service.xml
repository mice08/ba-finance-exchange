	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
	http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

    <bean id="monitorMailService" class="com.dianping.ba.finance.exchange.monitor.job.service.MonitorMailService">
        <property name="mailService" ref="mailService"/>
    </bean>

    <bean id="monitorSmsService" class="com.dianping.ba.finance.exchange.monitor.job.service.MonitorSmsService">
        <property name="smsService" ref="smsService"/>
    </bean>

    <bean id="scheduler" class="com.dianping.ba.finance.exchange.monitor.job.Scheduler">
        <property name="executorService" ref="executorService"/>
        <property name="pcMonitorService" ref="pcMonitorService"/>
