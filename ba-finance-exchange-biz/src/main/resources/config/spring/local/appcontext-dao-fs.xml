<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
	http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

    <bean id="exchangeOrderDao" parent="parentDao">
        <property name="proxyInterfaces" value="com.dianping.ba.finance.exchange.biz.dao.ExchangeOrderDao" />
        <property name="target">
            <bean parent="fsMain.daoRealizeTarget">
                <constructor-arg value="ExchangeOrder" />
            </bean>
        </property>
    </bean>

    <bean id="shopFundAccountDao" parent="parentDao">
        <property name="proxyInterfaces" value="com.dianping.ba.finance.exchange.biz.dao.ShopFundAccountDao" />
        <property name="target">
            <bean parent="fsMain.daoRealizeTarget">
                <constructor-arg value="ShopFundAccount" />
            </bean>
        </property>
    </bean>

    <bean id="shopFundAccountFlowDao" parent="parentDao">
        <property name="proxyInterfaces" value="com.dianping.ba.finance.exchange.biz.dao.ShopFundAccountFlowDao" />
        <property name="target">
            <bean parent="fsMain.daoRealizeTarget">
                <constructor-arg value="ShopFundAccountFlow" />
            </bean>
        </property>
    </bean>

    <bean id="payOrderDao" parent="parentDao">
        <property name="proxyInterfaces" value="com.dianping.ba.finance.exchange.biz.dao.PayOrderDao" />
        <property name="target">
            <bean parent="fsMain.daoRealizeTarget">
                <constructor-arg value="PayOrder" />
            </bean>
        </property>
    </bean>

    <bean id="receiveOrderDao" parent="parentDao">
        <property name="proxyInterfaces" value="com.dianping.ba.finance.exchange.biz.dao.ReceiveOrderDao" />
        <property name="target">
            <bean parent="fsMain.daoRealizeTarget">
                <constructor-arg value="ReceiveOrder" />
            </bean>
        </property>
    </bean>

    <bean id="receiveBankDao" parent="parentDao">
        <property name="proxyInterfaces" value="com.dianping.ba.finance.exchange.biz.dao.ReceiveBankDao" />
        <property name="target">
            <bean parent="fsMain.daoRealizeTarget">
                <constructor-arg value="ReceiveBank" />
            </bean>
        </property>
    </bean>

</beans>
