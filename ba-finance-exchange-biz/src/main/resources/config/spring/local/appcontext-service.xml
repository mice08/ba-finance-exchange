<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
	http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

    <bean id="exchangeOrderStatusChangeNotify" class="com.dianping.ba.finance.exchange.biz.producer.ExchangeOrderStatusChangeNotify">
        <property name="producerClient" ref="producerClient"/>
    </bean>

    <bean id="exchangeOrderService" class="com.dianping.ba.finance.exchange.biz.impl.ExchangeOrderServiceObject">
        <property name="exchangeOrderDao" ref="exchangeOrderDao"/>
        <property name="exchangeOrderStatusChangeNotify" ref="exchangeOrderStatusChangeNotify"/>
        <property name="threadPoolExecutor" ref="threadPoolExecutor"/>
    </bean>

    <bean id="shopFundAccountService" class="com.dianping.ba.finance.exchange.biz.impl.ShopFundAccountServiceObject">
        <property name="shopFundAccountFlowDao" ref="shopFundAccountFlowDao"/>
        <property name="shopFundAccountDao" ref="shopFundAccountDao"/>
        <property name="exchangeOrderService" ref="exchangeOrderService"/>
    </bean>

</beans>
