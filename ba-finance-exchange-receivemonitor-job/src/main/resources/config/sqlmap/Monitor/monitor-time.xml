<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="MonitorTime">
    <typeAlias alias="MonitorTimeData" type="com.dianping.ba.finance.exchange.receivemonitor.api.datas.MonitorTimeData"/>
    <resultMap id="monitorTimeData" class="MonitorTimeData">
        <result property="monitorId" column="MonitorID"/>
        <result property="monitorTime" column="MonitorTime"/>
    </resultMap>

    <insert id="insertMonitorTime" parameterClass="map">
        INSERT INTO FC_ReceiveMonitorTime
        (MonitorTime)
        VALUES (#monitorTimeData.monitorTime#)
        <selectKey resultClass="java.lang.Integer" keyProperty="MonitorID">
            SELECT @@IDENTITY AS id
        </selectKey>
    </insert>

    <select id="loadLastMonitorTime" parameterClass="map" resultMap="monitorTimeData">
        SELECT MonitorID,
                MonitorTime
          FROM FC_ReceiveMonitorTime
         WHERE MonitorID = (SELECT MAX(MonitorID) FROM FS_MonitorTime)
    </select>

</sqlMap>