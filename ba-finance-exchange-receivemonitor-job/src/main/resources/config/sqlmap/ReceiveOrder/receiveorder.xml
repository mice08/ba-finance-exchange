<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="ReceiveOrder">

    <typeAlias alias="ReceiveOrderMonitorData"
               type="com.dianping.ba.finance.exchange.receivemonitor.api.datas.ReceiveOrderMonitorData"></typeAlias>
    <resultMap id="ReceiveOrderMonitorData" class="ReceiveOrderMonitorData">
        <result property="roId" column="ROID"/>
        <result property="addDate" column="AddTime"/>
        <result property="status" column="Status"/>
        <result property="lastUpdateDate" column="UpdateTime"/>
    </resultMap>

    <select id="findReceiveOrderData" parameterClass="map" resultMap="ReceiveOrderMonitorData">
        SELECT ROID,
                Status,
                AddTime,
                UpdateTime
        FROM FC_ReceiveOrder
        <dynamic prepend="where">
            <isNotNull prepend="and" property="startDate">
                UpdateTime &gt;= #startDate#
            </isNotNull>
            <isNotNull prepend="and" property="endDate">
                UpdateTime &lt;= #endDate#
            </isNotNull>
        </dynamic>
    </select>

    <select id="loadReceiveOrderData" parameterClass="map" resultMap="ReceiveOrderMonitorData">
        SELECT ROID,
                Status,
                AddTime,
                UpdateTime
        FROM FC_ReceiveOrder
        WHERE ROID = #roId#
    </select>

</sqlMap>