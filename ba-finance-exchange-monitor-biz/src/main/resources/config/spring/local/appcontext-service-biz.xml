<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


    <bean id="exchangeOrderMonitorService" class="com.dianping.ba.finance.exchange.monitor.biz.impl.ExchangeOrderMonitorServiceObject">
        <property name="exchangeOrderMonitorDao" ref="exchangeOrderMonitorDao"/>
    </bean>

    <bean id="fsMonitorService" class="com.dianping.ba.finance.exchange.monitor.biz.impl.FSMonitorServiceObject">
        <property name="fsMonitorTimeDao" ref="fsMonitorTimeDao"/>
        <property name="fsMonitorTodoDao" ref="fsMonitorTodoDao"/>
        <property name="fsMonitorExceptionDao" ref="fsMonitorExceptionDao"/>
    </bean>

    <bean id="shopFundAccountFlowMonitorService" class="com.dianping.ba.finance.exchange.monitor.biz.impl.ShopFundAccountFlowMonitorServiceObject">
        <property name="shopFundAccountFlowMonitorDao" ref="shopFundAccountFlowMonitorDao"/>
    </bean>
</beans>
